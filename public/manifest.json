{
  "manifest_version": 3,
  "name": "Article Scraper Extension",
  "description": "Scrapes main article content from webpages",
  "version": "1.0",
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "sidePanel",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
} 